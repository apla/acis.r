echo '# this file generated by setup.ri_collections script'
echo '# '
echo 

for col in $metadata_collections; do
   eval colhome=\${metadata_${col}_home}
   eval coltype=\${metadata_${col}_type}
   eval colproc=\${metadata_${col}_proc}
   if test -z $colproc; then
      colproc='proc=ARDB::RI,records_db=1'
   fi
   echo $colhome $coltype $col $colproc
done


